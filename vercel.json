{
    "builds": [
      {
        "src": "CMR_Attendance_Module/wsgi.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
          "src": "/static/(.*)",
          "dest": "/static/$1"
      },
      {
          "src": "/(.*)",
          "dest": "CMR_Attendance_Module/wsgi.py"
      }
  ]
  }